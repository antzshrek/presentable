  "main": "index.js",
  "scripts": {
    "compodoc": "./node_modules/.bin/compodoc src -c .compodoc.json",
    "dev": "concurrently  \" npm run dev:server \" \" npm run dev:client \"  \" npm run dev:sync \"  \" npm run compodoc \" ",
    "dev:client": "ng build --watch --extract-css ",
    "dev:server": "nodemon",
    "dev:sync": "browser-sync start --config browser-sync.js",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "prestart:prod": "rimraf dist && tsc",
    "test": "jest",
    "test:cov": "jest --coverage",
    "test:watch": "jest --watch",
    "webpack": "webpack --config webpack.config.js"
  },
  "jest": {
    "coverageDirectory": "../coverage",
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testEnvironment": "node",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    }
  },
